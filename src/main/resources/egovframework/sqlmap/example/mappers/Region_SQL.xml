<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="RegionSpace">

	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
	<typeAlias  alias="RegionVO" type="egovframework.example.region.service.RegionVO"/>

	<select id="regionDAO.SelectRegionList" resultClass="RegionVO">
		SELECT pharm_no, pharm_name 
		  FROM tb_pharm
		ORDER BY pharm_no
		LIMIT 5;
	</select>

</sqlMap>